<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:background="@drawable/bg_general"
     >

    <ListView
        android:id="@+id/lsv_my_sign_up"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" >

    </ListView>

    <ImageButton
        android:id="@+id/btn_beam_sign_up"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="38dp"
        android:layout_marginBottom="35dp"
        android:background="@drawable/imagebutton_selector_touchsignin" />

    <ImageButton
        android:id="@+id/btn_ol_sign_up"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="41dp"
        android:layout_marginBottom="35dp"
        android:background="@drawable/imagebutton_selector_onlinesignin" />

</RelativeLayout>
