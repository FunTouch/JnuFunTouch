<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" 
    android:background="@drawable/background_usermenu2">

    <ImageButton
		    android:id="@+id/btn_nfc_flyer"
		    android:layout_width="115dp"
		    android:layout_height="34dp"
		    android:layout_above="@+id/btn_vote"
		    android:layout_alignLeft="@+id/btn_vote"
		    android:layout_marginTop="95dp"
		    android:layout_marginBottom="4dp"
		    android:background="@drawable/wuzhichuandan" />
	
	<ImageButton
	    android:id="@+id/btn_vote"
	    android:layout_width="115dp"
	    android:layout_height="34dp"
	    android:layout_above="@+id/btn_sign_up"
	    android:layout_alignLeft="@+id/btn_sign_up"
	    android:background="@drawable/faqitoupiao" 
	    />
	
	<ImageButton
	    android:id="@+id/btn_sign_up"
	    android:layout_width="115dp"
	    android:layout_height="34dp"
	    android:layout_above="@+id/btn_userMenu_Back"
	    android:layout_alignParentRight="true"
		android:layout_marginRight="45dp"
	    android:layout_marginBottom="151dp"
	    android:layout_marginTop="4dp"
	    android:background="@drawable/faqibaoming" />
	
	<ImageButton
	    android:id="@+id/btn_userMenu_Back"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentBottom="true"
	    android:layout_alignRight="@+id/btn_sign_up"
	    android:layout_marginBottom="45dp"
	    android:background="@drawable/fanhui" />
	    
</RelativeLayout>